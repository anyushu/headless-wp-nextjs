{
  "name": "headless-wp-nextjs",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next-sitemap",
    "start": "next start",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "prepare": "husky install",
    "gql-codegen": "graphql-codegen --require dotenv/config --config codegen.yml dotenv_config_path=.env.local"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "dom-parser-react": "^0.2.1",
    "graphql": "^16.3.0",
    "next": "^12.1.1",
    "next-seo": "^5.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.8",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "autoprefixer": "^10.4.4",
    "dotenv": "^16.0.0",
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-tailwindcss": "^3.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "next-sitemap": "^2.5.14",
    "postcss": "^8.4.12",
    "prettier": "^2.6.1",
    "tailwindcss": "^3.0.23",
    "typescript": "4.6.3"
  }
}
